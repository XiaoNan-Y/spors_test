Exemption.vue"],"sourcesContent":["<template>\r\n  <div class=\"exemption\">\r\n    <!-- 顶部说明卡片 -->\r\n    <el-card class=\"info-card\">\r\n      <div class=\"info-content\">\r\n        <i class=\"el-icon-info\"></i>\r\n        <div class=\"info-text\">\r\n          <h3>免测/重测申请</h3>\r\n          <p>如果您因特殊原因需要申请免测或重测，请在此提交申请。我们会尽快处理您的申请。</p>\r\n        </div>\r\n      </div>\r\n    </el-card>\r\n\r\n    <!-- 操作栏 -->\r\n    <div class=\"operation-bar\">\r\n      <el-button type=\"primary\" @click=\"handleSubmit\">提交申请</el-button>\r\n    </div>\r\n\r\n    <!-- 申请列表 -->\r\n    <el-table\r\n      :data=\"applicationList\"\r\n      border\r\n      style=\"width: 100%\"\r\n      v-loading=\"loading\"\r\n    >\r\n      <el-table-column prop=\"createTime\" label=\"申请时间\" width=\"180\">\r\n        <template slot-scope=\"scope\">\r\n          {{ formatDateTime(scope.row.createTime) }}\r\n        </template>\r\n      </el-table-column>\r\n      <el-table-column prop=\"type\" label=\"申请类型\" width=\"100\">\r\n        <template slot-scope=\"scope\">\r\n          <el-tag :type=\"getTypeTag(scope.row.type)\">\r\n            {{ scope.row.type === 'EXEMPTION' ? '免测' : '重测' }}\r\n          </el-tag>\r\n        </template>\r\n      </el-table-column>\r\n      <el-table-column prop=\"reason\" label=\"申请原因\" show-overflow-tooltip></el-table-column>\r\n      <el-table-column prop=\"status\" label=\"状态\" width=\"120\">\r\n        <template slot-scope=\"scope\">\r\n          <el-tag :type=\"getStatusType(scope.row.status)\">\r\n            {{ getStatusText(scope.row.status) }}\r\n          </el-tag>\r\n        </template>\r\n      </el-table-column>\r\n      <el-table-column prop=\"reviewComment\" label=\"审核意见\" show-overflow-tooltip></el-table-column>\r\n      <el-table-column label=\"操作\" width=\"150\" fixed=\"right\">\r\n        <template slot-scope=\"scope\">\r\n          <el-button-group>\r\n            <el-button\r\n              type=\"primary\"\r\n              size=\"mini\"\r\n              @click=\"handleEdit(scope.row)\"\r\n              :disabled=\"!canEdit(scope.row)\"\r\n            >\r\n              修改\r\n            </el-button>\r\n            <el-button\r\n              type=\"danger\"\r\n              size=\"mini\"\r\n              @click=\"handleCancel(scope.row)\"\r\n              :disabled=\"!canCancel(scope.row)\"\r\n            >\r\n              撤销\r\n            </el-button>\r\n          </el-button-group>\r\n        </template>\r\n      </el-table-column>\r\n    </el-table>\r\n\r\n    <!-- 提交/修改申请对话框 -->\r\n    <el-dialog\r\n      :title=\"editingId ? '修改申请' : '提交申请'\"\r\n      :visible.sync=\"dialogVisible\"\r\n      width=\"500px\"\r\n      :close-on-click-modal=\"false\"\r\n    >\r\n      <el-form :model=\"form\" :rules=\"rules\" ref=\"form\" label-width=\"100px\">\r\n        <el-form-item label=\"申请类型\" prop=\"type\">\r\n          <el-radio-group v-model=\"form.type\">\r\n            <el-radio label=\"EXEMPTION\">免测</el-radio>\r\n            <el-radio label=\"RETEST\">重测</el-radio>\r\n          </el-radio-group>\r\n        </el-form-item>\r\n\r\n        <!-- 当选择重测时显示体测项目选择 -->\r\n        <el-form-item \r\n          v-if=\"form.type === 'RETEST'\" \r\n          label=\"体测项目\" \r\n          prop=\"sportsItemId\"\r\n        >\r\n          <el-select \r\n            v-model=\"form.sportsItemId\" \r\n            placeholder=\"请选择体测项目\"\r\n            style=\"width: 100%\"\r\n          >\r\n            <el-option\r\n              v-for=\"item in sportsItems\"\r\n              :key=\"item.id\"\r\n              :label=\"item.name\"\r\n              :value=\"item.id\"\r\n            ></el-option>\r\n          </el-select>\r\n        </el-form-item>\r\n\r\n        <el-form-item label=\"申请原因\" prop=\"reason\">\r\n          <el-input\r\n            type=\"textarea\"\r\n            v-model=\"form.reason\"\r\n            :rows=\"4\"\r\n            placeholder=\"请详细说明申请原因\"\r\n          ></el-input>\r\n        </el-form-item>\r\n      </el-form>\r\n      <div slot=\"footer\" class=\"dialog-footer\">\r\n        <el-button @click=\"dialogVisible = false\">取 消</el-button>\r\n        <el-button type=\"primary\" @click=\"submitForm\">确 定</el-button>\r\n      </div>\r\n    </el-dialog>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: 'Exemption',\r\n  data() {\r\n    return {\r\n      loading: false,\r\n      dialogVisible: false,\r\n      applicationList: [],\r\n      sportsItems: [],\r\n      editingId: null,\r\n      form: {\r\n        type: 'EXEMPTION',\r\n        reason: '',\r\n        sportsItemId: null\r\n      },\r\n      rules: {\r\n        type: [{ required: true, message: '请选择申请类型', trigger: 'change' }],\r\n        reason: [{ required: true, message: '请输入申请原因', trigger: 'blur' }],\r\n        sportsItemId: [{ \r\n          required: true, \r\n          message: '请选择体测项目', \r\n          trigger: 'change',\r\n          validator: (rule, value, callback) => {\r\n            if (this.form.type === 'RETEST' && !value) {\r\n              callback(new Error('请选择体测项目'))\r\n            } else {\r\n              callback()\r\n            }\r\n          }\r\n        }]\r\n      }\r\n    }\r\n  },\r\n  created() {\r\n    this.fetchApplications()\r\n    this.fetchSportsItems()\r\n  },\r\n  methods: {\r\n    async fetchApplications() {\r\n      try {\r\n        this.loading = true;\r\n        console.log('Fetching applications...');\r\n        \r\n        const res = await this.$http.get('/api/student/exemptions', {\r\n          params: {\r\n            page: 0,\r\n            size: 10\r\n          },\r\n          headers: {\r\n            'Authorization': `Bearer ${this.$store.state.userId}` // 添加用户ID\r\n          }\r\n        });\r\n        \r\n        console.log('API response:', res.data);\r\n        \r\n        if (res.data.code === 200) {\r\n          this.applicationList = res.data.data.content;\r\n          console.log('Applications loaded:', this.applicationList);\r\n        } else {\r\n          this.$message.error(res.data.message || '获取申请列表失败');\r\n        }\r\n      } catch (error) {\r\n        console.error('获取申请列表失败:', error);\r\n        this.$message.error('获取申请列表失败');\r\n      } finally {\r\n        this.loading = false;\r\n      }\r\n    },\r\n    async fetchSportsItems() {\r\n      try {\r\n        const res = await this.$http.get('/api/student/sports-items')\r\n        if (res.data.code === 200) {\r\n          this.sportsItems = res.data.data\r\n        }\r\n      } catch (error) {\r\n        console.error('获取体测项目列表失败:', error)\r\n        this.$message.error('获取体测项目列表失败')\r\n      }\r\n    },\r\n    handleSubmit() {\r\n      this.editingId = null\r\n      this.form = {\r\n        type: 'EXEMPTION',\r\n        reason: '',\r\n        sportsItemId: null\r\n      }\r\n      this.dialogVisible = true\r\n    },\r\n    async submitForm() {\r\n      this.$refs.form.validate(async valid => {\r\n        if (valid) {\r\n          try {\r\n            const url = this.editingId \r\n              ? `/api/student/exemptions/${this.editingId}`\r\n              : '/api/student/exemptions'\r\n            const method = this.editingId ? 'put' : 'post'\r\n            \r\n            const res = await this.$http[method](url, this.form)\r\n            \r\n            if (res.data.code === 200) {\r\n              this.$message.success(this.editingId ? '修改成功' : '提交成功')\r\n              this.dialogVisible = false\r\n              this.fetchApplications()\r\n            }\r\n          } catch (error) {\r\n            console.error('提交失败:', error)\r\n            this.$message.error('提交失败')\r\n          }\r\n        }\r\n      })\r\n    },\r\n    handleEdit(row) {\r\n      this.editingId = row.id\r\n      this.form = {\r\n        type: row.type,\r\n        reason: row.reason,\r\n        sportsItemId: row.sportsItemId\r\n      }\r\n      this.dialogVisible = true\r\n    },\r\n    async handleCancel(row) {\r\n      try {\r\n        await this.$confirm('确认撤销该申请吗？', '提示', {\r\n          confirmButtonText: '确定',\r\n          cancelButtonText: '取消',\r\n          type: 'warning'\r\n        })\r\n        \r\n        const res = await this.$http.delete(`/api/student/exemptions/${row.id}`)\r\n        if (res.data.code === 200) {\r\n          this.$message.success('撤销成功')\r\n          this.fetchApplications()\r\n        }\r\n      } catch (error) {\r\n        if (error !== 'cancel') {\r\n          console.error('撤销失败:', error)\r\n          this.$message.error('撤销失败')\r\n        }\r\n      }\r\n    },\r\n    getTypeTag(type) {\r\n      return type === 'EXEMPTION' ? 'warning' : 'primary'\r\n    },\r\n    getStatusType(status) {\r\n      switch (status) {\r\n        case 'PENDING_TEACHER': return 'info'\r\n        case 'PENDING_ADMIN': return 'warning'\r\n        case 'APPROVED': return 'success'\r\n        case 'REJECTED_TEACHER':\r\n        case 'REJECTED': return 'danger'\r\n        default: return 'info'\r\n      }\r\n    },\r\n    getStatusText(status) {\r\n      switch (status) {\r\n        case 'PENDING_TEACHER': return '待教师审核'\r\n        case 'PENDING_ADMIN': return '待管理员审核'\r\n        case 'APPROVED': return '已通过'\r\n        case 'REJECTED_TEACHER': return '教师已驳回'\r\n        case 'REJECTED': return '已驳回'\r\n        default: return status\r\n      }\r\n    },\r\n    formatDateTime(datetime) {\r\n      if (!datetime) return ''\r\n      return new Date(datetime).toLocaleString()\r\n    },\r\n    canEdit(row) {\r\n      return row.status === 'PENDING_TEACHER'\r\n    },\r\n    canCancel(row) {\r\n      return row.status === 'PENDING_TEACHER'\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n.exemption {\r\n  padding: 20px;\r\n\r\n  .info-card {\r\n    margin-bottom: 20px;\r\n    \r\n    .info-content {\r\n      display: flex;\r\n      align-items: flex-start;\r\n\r\n      .el-icon-info {\r\n        font-size: 24px;\r\n        color: #409EFF;\r\n        margin-right: 15px;\r\n      }\r\n\r\n      .info-text {\r\n        h3 {\r\n          margin: 0 0 10px;\r\n          color: #303133;\r\n        }\r\n\r\n        p {\r\n          margin: 0;\r\n          color: #606266;\r\n        }\r\n      }\r\n    }\r\n  }\r\n\r\n  .operation-bar {\r\n    margin: 20px 0;\r\n  }\r\n}\r\n</style> "],"mappings":";AA2HA;EACAA,IAAA;EACAC,KAAA;IACA;MACAC,OAAA;MACAC,aAAA;MACAC,eAAA;MACAC,WAAA;MACAC,SAAA;MACAC,IAAA;QACAC,IAAA;QACAC,MAAA;QACAC,YAAA;MACA;MACAC,KAAA;QACAH,IAAA;UAAAI,QAAA;UAAAC,OAAA;UAAAC,OAAA;QAAA;QACAL,MAAA;UAAAG,QAAA;UAAAC,OAAA;UAAAC,OAAA;QAAA;QACAJ,YAAA;UACAE,QAAA;UACAC,OAAA;UACAC,OAAA;UACAC,SAAA,EAAAA,CAAAC,IAAA,EAAAC,KAAA,EAAAC,QAAA;YACA,SAAAX,IAAA,CAAAC,IAAA,kBAAAS,KAAA;cACAC,QAAA,KAAAC,KAAA;YACA;cACAD,QAAA;YACA;UACA;QACA;MACA;IACA;EACA;EACAE,QAAA;IACA,KAAAC,iBAAA;IACA,KAAAC,gBAAA;EACA;EACAC,OAAA;IACA,MAAAF,kBAAA;MACA;QACA,KAAAnB,OAAA;QACAsB,OAAA,CAAAC,GAAA;QAEA,MAAAC,GAAA,cAAAC,KAAA,CAAAC,GAAA;UACAC,MAAA;YACAC,IAAA;YACAC,IAAA;UACA;UACAC,OAAA;YACA,gCAAAC,MAAA,CAAAC,KAAA,CAAAC,MAAA;UACA;QACA;QAEAX,OAAA,CAAAC,GAAA,kBAAAC,GAAA,CAAAzB,IAAA;QAEA,IAAAyB,GAAA,CAAAzB,IAAA,CAAAmC,IAAA;UACA,KAAAhC,eAAA,GAAAsB,GAAA,CAAAzB,IAAA,CAAAA,IAAA,CAAAoC,OAAA;UACAb,OAAA,CAAAC,GAAA,8BAAArB,eAAA;QACA;UACA,KAAAkC,QAAA,CAAAC,KAAA,CAAAb,GAAA,CAAAzB,IAAA,CAAAY,OAAA;QACA;MACA,SAAA0B,KAAA;QACAf,OAAA,CAAAe,KAAA,cAAAA,KAAA;QACA,KAAAD,QAAA,CAAAC,KAAA;MACA;QACA,KAAArC,OAAA;MACA;IACA;IACA,MAAAoB,iBAAA;MACA;QACA,MAAAI,GAAA,cAAAC,KAAA,CAAAC,GAAA;QACA,IAAAF,GAAA,CAAAzB,IAAA,CAAAmC,IAAA;UACA,KAAA/B,WAAA,GAAAqB,GAAA,CAAAzB,IAAA,CAAAA,IAAA;QACA;MACA,SAAAsC,KAAA;QACAf,OAAA,CAAAe,KAAA,gBAAAA,KAAA;QACA,KAAAD,QAAA,CAAAC,KAAA;MACA;IACA;IACAC,aAAA;MACA,KAAAlC,SAAA;MACA,KAAAC,IAAA;QACAC,IAAA;QACAC,MAAA;QACAC,YAAA;MACA;MACA,KAAAP,aAAA;IACA;IACA,MAAAsC,WAAA;MACA,KAAAC,KAAA,CAAAnC,IAAA,CAAAoC,QAAA,OAAAC,KAAA;QACA,IAAAA,KAAA;UACA;YACA,MAAAC,GAAA,QAAAvC,SAAA,GACA,gCAAAA,SAAA,KACA;YACA,MAAAwC,MAAA,QAAAxC,SAAA;YAEA,MAAAoB,GAAA,cAAAC,KAAA,CAAAmB,MAAA,EAAAD,GAAA,OAAAtC,IAAA;YAEA,IAAAmB,GAAA,CAAAzB,IAAA,CAAAmC,IAAA;cACA,