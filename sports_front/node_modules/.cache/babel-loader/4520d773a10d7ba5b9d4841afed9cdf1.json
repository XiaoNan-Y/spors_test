  })\r\n    \r\n    if (token) {\r\n      config.headers['Authorization'] = `Bearer ${token}`\r\n    }\r\n    if (userId) {\r\n      config.headers['userId'] = userId\r\n      console.log('设置请求头 userId:', userId)\r\n    }\r\n    \r\n    // 从localStorage获取用户ID\r\n    const userInfo = JSON.parse(localStorage.getItem('studentInfo'));\r\n    if (userInfo && userInfo.id) {\r\n      // 添加到请求头\r\n      config.headers['X-User-ID'] = userInfo.id;\r\n    }\r\n    \r\n    // 打印最终的请求配置\r\n    console.log('最终请求配置:', {\r\n      url: config.url,\r\n      method: config.method,\r\n      headers: config.headers\r\n    })\r\n    \r\n    return config\r\n  },\r\n  error => {\r\n    console.error('请求错误:', error)\r\n    return Promise.reject(error)\r\n  }\r\n)\r\n\r\n// 响应拦截器\r\nrequest.interceptors.response.use(\r\n  response => {\r\n    const res = response.data\r\n    \r\n    // 添加调试日志\r\n    console.log('Response data:', res)\r\n    \r\n    if (res.code !== 200) {\r\n      Message({\r\n        message: res.message || res.msg || '错误',\r\n        type: 'error',\r\n        duration: 5 * 1000\r\n      })\r\n      return Promise.reject(res.message || res.msg || '错误')\r\n    }\r\n    return response\r\n  },\r\n  error => {\r\n    console.error('Response error:', error)\r\n    if (error.response && error.response.status === 401) {\r\n      // 清除本地存储的用户信息\r\n      localStorage.clear()\r\n      // 重定向到登录页\r\n      window.location.href = '/login'\r\n    }\r\n    Message.error(error.response?.data?.message || '请求失败')\r\n    return Promise.reject(error)\r\n  }\r\n)\r\n\r\nexport default request "],"mappings":"AAAA,OAAOA,KAAK,MAAM,OAAO;AACzB,SAASC,OAAO,QAAQ,YAAY;;AAEpC;AACA,MAAMC,OAAO,GAAGF,KAAK,CAACG,MAAM,CAAC;EAC3BC,OAAO,EAAEC,OAAO,CAACC,GAAG,CAACC,eAAe,IAAI,uBAAuB;EAAE;EACjEC,OAAO,EAAE,IAAI,CAAC;AAChB,CAAC,CAAC;;AAEF;AACAN,OAAO,CAACO,YAAY,CAACP,OAAO,CAACQ,GAAG,CAC9BC,MAAM,IAAI;EACR;EACA,MAAMC,KAAK,GAAGC,YAAY,CAACC,OAAO,CAAC,OAAO,CAAC;EAC3C,MAAMC,MAAM,GAAGF,YAAY,CAACC,OAAO,CAAC,QAAQ,CAAC;EAC7C