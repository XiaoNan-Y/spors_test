oken}`\n    }\n    if (userId) {\n      config.headers['userId'] = userId\n      console.log('设置请求头 userId:', userId)\n    }\n    \n    // 打印最终的请求配置\n    console.log('最终请求配置:', {\n      url: config.url,\n      method: config.method,\n      headers: config.headers\n    })\n    \n    return config\n  },\n  error => {\n    console.error('请求错误:', error)\n    return Promise.reject(error)\n  }\n)\n\n// 响应拦截器\nrequest.interceptors.response.use(\n  response => {\n    const res = response.data\n    \n    // 添加调试日志\n    console.log('Response data:', res)\n    \n    if (res.code !== 200) {\n      Message({\n        message: res.message || res.msg || '错误',\n        type: 'error',\n        duration: 5 * 1000\n      })\n      return Promise.reject(res.message || res.msg || '错误')\n    }\n    return response\n  },\n  error => {\n    console.error('Response error:', error)\n    if (error.response && error.response.status === 401) {\n      // 清除本地存储的用户信息\n      localStorage.clear()\n      // 重定向到登录页\n      window.location.href = '/login'\n    }\n    Message.error(error.response?.data?.message || '请求失败')\n    return Promise.reject(error)\n  }\n)\n\nexport default request "],"mappings":"AAAA,OAAOA,KAAK,MAAM,OAAO;AACzB,SAASC,OAAO,QAAQ,YAAY;;AAEpC;AACA,MAAMC,OAAO,GAAGF,KAAK,CAACG,MAAM,CAAC;EAC3BC,OAAO,EAAEC,OAAO,CAACC,GAAG,CAACC,eAAe,IAAI,uBAAuB;EAAE;EACjEC,OAAO,EAAE,IAAI,CAAC;AAChB,CAAC,CAAC;;AAEF;AACAN,OAAO,CAACO,YAAY,CAACP,OAAO,CAACQ,GAAG,CAC9BC,MAAM,IAAI;EACR;EACA,MAAMC,KAAK,GAAGC,YAAY,CAACC,OAAO,CAAC,OAAO,CAAC;EAC3C,MAAMC,MAAM,GAAGF,YAAY,CAACC,OAAO,CAAC,QAAQ,CAAC;EAC7C,MAAME,QAAQ,GAAGH,YAAY,CAACC,OAAO,CAAC,UAAU,CAAC;;EAEjD;EACAG,OAAO,CAACC,GAAG,CAAC,QAAQ,EAAE;IACpBC,GAAG,EAAER,MAAM,CAACQ,GAAG;IACfC,MAAM,EAAET,MAAM,CAACS,MAAM;IACrBR,KAAK;IACLG,MAAM;IACNC,QAAQ;IACRK,eAAe,EAAEV,MAAM,CAACW;EAC1B,CAAC,CAAC;EAEF,IAAIV,KAAK,EAAE;IACTD,MAAM,CAACW,OAAO,CAAC,eAAe,CAAC,GAAG,UAAUV,KAAK,EAAE;EACrD;EACA,IAAIG,MAAM,EAAE;IACVJ,MAAM,CAACW,OAAO,CAAC,QAAQ,CAAC,GAAGP,MAAM;IACjCE,OAAO,CAACC,GAAG,CAAC,eA