rces":["src/layouts/TeacherLayout.vue"],"sourcesContent":["<template>\n  <el-container class=\"teacher-layout\">\n    <el-aside width=\"200px\">\n      <el-menu\n        :default-active=\"activeMenu\"\n        class=\"el-menu-vertical\"\n        background-color=\"#304156\"\n        text-color=\"#bfcbd9\"\n        active-text-color=\"#409EFF\"\n        :router=\"true\"\n      >\n        <el-menu-item index=\"/teacher/dashboard\">\n          <i class=\"el-icon-s-home\"></i>\n          <span slot=\"title\">首页</span>\n        </el-menu-item>\n\n        <el-menu-item index=\"/teacher/class-stats\">\n          <i class=\"el-icon-s-data\"></i>\n          <span slot=\"title\">班级成绩统计</span>\n        </el-menu-item>\n\n        <el-menu-item index=\"/teacher/student-records\">\n          <i class=\"el-icon-document\"></i>\n          <span slot=\"title\">学生成绩管理</span>\n        </el-menu-item>\n\n        <el-menu-item index=\"/teacher/exemption-review\">\n          <i class=\"el-icon-document-checked\"></i>\n          <span slot=\"title\">免测/重测审核</span>\n        </el-menu-item>\n\n        <el-menu-item index=\"/teacher/student-management\">\n          <i class=\"el-icon-user-solid\"></i>\n          <span slot=\"title\">学生管理</span>\n        </el-menu-item>\n\n        <el-menu-item index=\"/teacher/notice-management\">\n          <i class=\"el-icon-bell\"></i>\n          <span slot=\"title\">通知管理</span>\n        </el-menu-item>\n\n        <el-menu-item index=\"/teacher/profile\">\n          <i class=\"el-icon-user\"></i>\n          <span slot=\"title\">个人信息</span>\n        </el-menu-item>\n      </el-menu>\n    </el-aside>\n    \n    <el-container>\n      <el-header>\n        <div class=\"header-left\">体育测试管理系统-教师端</div>\n        <div class=\"header-right\">\n          <el-dropdown @command=\"handleCommand\">\n            <span class=\"el-dropdown-link\">\n              {{ username }}<i class=\"el-icon-arrow-down el-icon--right\"></i>\n            </span>\n            <el-dropdown-menu slot=\"dropdown\">\n              <el-dropdown-item command=\"profile\">个人信息</el-dropdown-item>\n              <el-dropdown-item command=\"logout\">退出登录</el-dropdown-item>\n            </el-dropdown-menu>\n          </el-dropdown>\n        </div>\n      </el-header>\n      \n      <el-main>\n        <router-view></router-view>\n      </el-main>\n    </el-container>\n  </el-container>\n</template>\n\n<script>\nexport default {\n  name: 'TeacherLayout',\n  data() {\n    return {\n      username: localStorage.getItem('username') || '教师',\n      activeMenu: ''\n    }\n  },\n  created() {\n    // 检查用户信息\n    const userId = localStorage.getItem('userId')\n    const userRole = localStorage.getItem('userRole')\n    const token = localStorage.getItem('token')\n    \n    console.log('TeacherLayout created - 用户信息:', {\n      userId,\n      userRole,\n      token\n    })\n    \n    if (!token || !userId || !userRole || userRole !== 'TEACHER') {\n      console.log('用户信息无效，重定向到登录页')\n      localStorage.clear() // 清除可能存在的无效信息\n      this.$router.push('/login')\n    }\n    this.activeMenu = this.$route.path\n  },\n  methods: {\n    handleCommand(command) {\n      if (command === 'logout') {\n        // 清除所有本地存储数据\n        localStorage.clear()\n        \n        // 使用 $nextTick 确保状态更新\n        this.$nextTick(async () => {\n          try {\n            // 跳转到登录页\n            await this.$router.push('/login')\n            \n            // 显示退出成功消息\n            this.$message.success('已退出登录')\n          } catch (error) {\n            console.error('退出登录时发生错误:', error)\n          }\n        })\n      } else if (command === 'profile') {\n        this.$router.push('/teacher/profile')\n      }\n    }\n  }\n}\n</script>\n\n<style scoped>\n.teacher-layout {\n  height: 100vh;\n}\n\n.el-aside {\n  background-color: #304156;\n}\n\n.el-menu {\n  border-right: none;\n}\n\n.el-header {\n  background-color: #fff;\n  color: #333;\n  display: flex;\n  justify-content: space-between;\n  align-items: center;\n  border-bottom: 1px solid #ddd;\n}\n\n.header-left {\n  margin-left: 20px;\n}\n\n.header-right {\n  display: flex;\n  align-items: center;\n}\n\n.header-right span {\n  margin-right: 15px;\n}\n\n.el-main {\n  background-color: #f0f2f5;\n  padding: 20px;\n}\n</style>"],"mappings":";AAwEA;EACAA,IAAA;EACAC,KAAA;IACA;MACAC,QAAA,EAAAC,YAAA,CAAAC,OAAA;MACAC,UAAA;IACA;EACA;EACAC,QAAA;IACA;IACA,MAAAC,MAAA,GAAAJ,YAAA,CAAAC,OAAA;IACA,MAAAI,QAAA,GAAAL,YAAA,CAAAC,OAAA;IACA,MAAAK,KAAA,GAAAN,YAAA,CAAAC,OAAA;IAEAM,OAAA,CAAAC,GAAA;MACAJ,MAAA;MACAC,QAAA;MACAC;IACA;IAEA,KAAAA,KAAA,KAAAF,MAAA,KAAAC,QAAA,IAAAA,QAAA;MACAE,OAAA,CAAAC,GAAA;MACAR,YAAA,CAAAS,KAAA;MACA,KAAAC,OAAA,CAAAC,IAAA;IACA;IACA,KAAAT,UAAA,QAAAU,MAAA,CAAAC,IAAA;EACA;EACAC,OAAA;IACAC,cAAAC,OAAA;MACA,IAAAA,OAAA;QACA;QACAhB,YAAA,CAAAS,KAAA;;QAEA;QACA,KAAAQ,SAAA;UACA;YACA;YACA,WAAAP,OAAA,CAAAC,IAAA;;YAEA;YACA,KAAAO,QAAA,CAAAC,OAAA;UACA,SAAAC,KAAA;YACAb,OAAA,CAAAa,KAAA,eAAAA,KAAA;UACA;QACA;MACA,WAAAJ,OAAA;QACA,KAAAN,OAAA,CAAAC,IAAA;MACA;IACA;EACA;AACA","ignoreList":[]}]}