an>{{ record.student ? record.student.realName : '' }}</span>\r\n        </el-form-item>\r\n        <el-form-item label=\"学号\">\r\n          <span>{{ record.studentNumber }}</span>\r\n        </el-form-item>\r\n        <el-form-item label=\"测试项目\">\r\n          <span>{{ record.sportsItem ? record.sportsItem.name : '' }}</span>\r\n        </el-form-item>\r\n        <el-form-item label=\"测试成绩\">\r\n          <span>{{ record.score }}</span>\r\n        </el-form-item>\r\n        <el-form-item label=\"测试时间\">\r\n          <span>{{ formatDateTime(record.testTime) }}</span>\r\n        </el-form-item>\r\n        <el-form-item label=\"审核状态\" prop=\"status\">\r\n          <el-radio-group v-model=\"form.status\">\r\n            <el-radio label=\"APPROVED\">通过</el-radio>\r\n            <el-radio label=\"REJECTED\">驳回</el-radio>\r\n          </el-radio-group>\r\n        </el-form-item>\r\n        <el-form-item label=\"审核意见\" prop=\"comment\">\r\n          <el-input\r\n            type=\"textarea\"\r\n            v-model=\"form.comment\"\r\n            :rows=\"3\"\r\n            placeholder=\"请输入审核意见\">\r\n          </el-input>\r\n        </el-form-item>\r\n      </el-form>\r\n      <div slot=\"footer\" class=\"dialog-footer\">\r\n        <el-button @click=\"handleClose\">取 消</el-button>\r\n        <el-button type=\"primary\" @click=\"handleSubmit\">确 定</el-button>\r\n      </div>\r\n    </el-dialog>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { formatDateTime } from '@/utils/datetime'\r\n\r\nexport default {\r\n  name: 'TestRecordReview',\r\n  props: {\r\n    visible: {\r\n      type: Boolean,\r\n      default: false\r\n    },\r\n    record: {\r\n      type: Object,\r\n      default: () => ({})\r\n    }\r\n  },\r\n  data() {\r\n    return {\r\n      form: {\r\n        status: '',\r\n        comment: ''\r\n      }\r\n    }\r\n  },\r\n  methods: {\r\n    formatDateTime,\r\n    handleClose() {\r\n      this.$emit('update:visible', false)\r\n      this.resetForm()\r\n    },\r\n    resetForm() {\r\n      this.form = {\r\n        status: '',\r\n        comment: ''\r\n      }\r\n    },\r\n    async handleSubmit() {\r\n      try {\r\n        const response = await this.$axios.put(`/api/admin/test-record/review`, {\r\n          id: this.record.id,\r\n          status: this.form.status,\r\n          comment: this.form.comment,\r\n          reviewerId: this.$store.state.user.id // 假设存储了当前用户信息\r\n        })\r\n        \r\n        if (response.data.code === 200) {\r\n          this.$message.success('审核成功')\r\n          this.$emit('success')\r\n          this.handleClose()\r\n        } else {\r\n          this.$message.error(response.data.msg || '审核失败')\r\n        }\r\n      } catch (error) {\r\n        this.$message.error('审核失败：' + error.message)\r\n      }\r\n    }\r\n  },\r\n  watch: {\r\n    record: {\r\n      handler(newVal) {\r\n        if (newVal) {\r\n          this.form.status = ''\r\n          this.form.comment = ''\r\n        }\r\n      },\r\n      immediate: true\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.test-record-review {\r\n  .el-form-item {\r\n    margin-bottom: 20px;\r\n  }\r\n}\r\n</style> "],"mappings":"AA+CA,SAAAA,cAAA;AAEA;EACAC,IAAA;EACAC,KAAA;IACAC,OAAA;MACAC,IAAA,EAAAC,OAAA;MACAC,OAAA;IACA;IACAC,MAAA;MACAH,IAAA,EAAAI,MAAA;MACAF,OAAA,EAAAA,CAAA;IACA;EACA;EACAG,KAAA;IACA;MACAC,IAAA;QACAC,MAAA;QACAC,OAAA;MACA;IACA;EACA;EACAC,OAAA;IACAb,cAAA;IACAc,YAAA;MACA,KAAAC,KAAA;MACA,KAAAC,SAAA;IACA;IACAA,UAAA;MACA,KAAAN,IAAA;QACAC,MAAA;QACAC,OAAA;MACA;IACA;IACA,MAAAK,aAAA;MACA;QACA,MAAAC,QAAA,cAAAC,MAAA,CAAAC,GAAA;UACAC,EAAA,OAAAd,MAAA,CAAAc,EAAA;UACAV,MAAA,OAAAD,IAAA,CAAAC,MAAA;UACAC,OAAA,OAAAF,IAAA,CAAAE,OAAA;UACAU,UAAA,OAAAC,MAAA,CAAAC,KAAA,CAAAC,IAAA,CAAAJ,EAAA;QACA;QAEA,IAAAH,QAAA,CAAAT,IAAA,CAAAiB,IAAA;UACA,KAAAC,QAAA,CAAAC,OAAA;UACA,KAAAb,KAAA;UACA,KAAAD,WAAA;QACA;UACA,KAAAa,QAAA,CAAAE,KAAA,CAAAX,QAAA,CAAAT,IAAA,CAAAqB,GAAA;QACA;MACA,SAAAD,KAAA;QACA,KAAAF,QAAA,CAAAE,KAAA,WAAAA,KAAA,CAAAE,OAAA;MACA;IACA;EACA;E