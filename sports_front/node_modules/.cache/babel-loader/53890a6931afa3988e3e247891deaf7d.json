">\r\n    <div class=\"operation-bar\">\r\n      <div class=\"search-area\">\r\n        <el-input\r\n          v-model=\"searchKeyword\"\r\n          placeholder=\"输入项目名称搜索\"\r\n          style=\"width: 200px\"\r\n          @keyup.enter.native=\"handleSearch\"\r\n        >\r\n          <el-button slot=\"append\" icon=\"el-icon-search\" @click=\"handleSearch\"></el-button>\r\n        </el-input>\r\n      </div>\r\n      \r\n      <el-button type=\"primary\" @click=\"handleAdd\">\r\n        <i class=\"el-icon-plus\"></i> 添加项目\r\n      </el-button>\r\n    </div>\r\n\r\n    <el-table\r\n      :data=\"tableData || []\"\r\n      border\r\n      stripe\r\n      style=\"width: 100%\"\r\n      v-loading=\"loading\"\r\n    >\r\n      <el-table-column prop=\"id\" label=\"ID\" width=\"80\"></el-table-column>\r\n      <el-table-column prop=\"name\" label=\"项目名称\"></el-table-column>\r\n      <el-table-column prop=\"type\" label=\"项目类型\" width=\"100\"></el-table-column>\r\n      <el-table-column prop=\"description\" label=\"项目描述\" show-overflow-tooltip></el-table-column>\r\n      <el-table-column prop=\"unit\" label=\"计量单位\" width=\"100\"></el-table-column>\r\n      <el-table-column prop=\"isActive\" label=\"状态\" width=\"100\">\r\n        <template slot-scope=\"scope\">\r\n          <el-switch\r\n            v-model=\"scope.row.isActive\"\r\n            @change=\"handleStatusChange(scope.row)\"\r\n            active-color=\"#13ce66\"\r\n            inactive-color=\"#ff4949\"\r\n          >\r\n          </el-switch>\r\n        </template>\r\n      </el-table-column>\r\n      <el-table-column label=\"操作\" width=\"200\">\r\n        <template slot-scope=\"scope\">\r\n          <el-button-group>\r\n            <el-button\r\n              size=\"mini\"\r\n              type=\"primary\"\r\n              @click=\"handleEdit(scope.row)\"\r\n            >\r\n              编辑\r\n            </el-button>\r\n            <el-button\r\n              size=\"mini\"\r\n              type=\"danger\"\r\n              @click=\"handleDelete(scope.row)\"\r\n            >\r\n              删除\r\n            </el-button>\r\n          </el-button-group>\r\n        </template>\r\n      </el-table-column>\r\n    </el-table>\r\n\r\n    <!-- 分页 -->\r\n    <div class=\"pagination-container\">\r\n      <el-pagination\r\n        @size-change=\"handleSizeChange\"\r\n        @current-change=\"handleCurrentChange\"\r\n        :current-page=\"currentPage\"\r\n        :page-sizes=\"[10, 20, 50, 100]\"\r\n        :page-size=\"pageSize\"\r\n        layout=\"total, sizes, prev, pager, next, jumper\"\r\n        :total=\"total\"\r\n      >\r\n      </el-pagination>\r\n    </div>\r\n\r\n    <!-- 添加/编辑对话框 -->\r\n    <el-dialog :title=\"dialogTitle\" :visible.sync=\"dialogVisible\" width=\"500px\">\r\n      <el-form :model=\"form\" :rules=\"rules\" ref=\"form\" label-width=\"100px\">\r\n        <el-form-item label=\"项目名称\" prop=\"name\">\r\n          <el-input v-model=\"form.name\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"项目类型\" prop=\"type\">\r\n          <el-select v-model=\"form.type\" placeholder=\"请选择项目类型\">\r\n            <el-option label=\"田径\" value=\"田径\"></el-option>\r\n            <el-option label=\"力量\" value=\"力量\"></el-option>\r\n          </el-select>\r\n        </el-form-item>\r\n        <el-form-item label=\"项目描述\" prop=\"description\">\r\n          <el-input type=\"textarea\" v-model=\"form.description\" rows=\"3\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"计量单位\" prop=\"unit\">\r\n          <el-input v-model=\"form.unit\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"状态\" prop=\"isActive\">\r\n          <el-switch v-model=\"form.isActive\"></el-switch>\r\n        </el-form-item>\r\n      </el-form>\r\n      <div slot=\"footer\">\r\n        <el-button @click=\"dialogVisible = false\">取消</el-button>\r\n        <el-button type=\"primary\" @click=\"handleSubmit\">确定</el-button>\r\n      </div>\r\n    </el-dialog>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: 'SportsItemManagement',\r\n  data() {\r\n    return {\r\n      searchKeyword: '',\r\n      tableData: [],\r\n      loading: false,\r\n      currentPage: 1,\r\n      pageSize: 10,\r\n      total: 0,\r\n      dialogVisible: false,\r\n      dialogTitle: '',\r\n      form: {\r\n        name: '',\r\n        type: '',\r\n        description: '',\r\n        unit: '',\r\n        isActive: true\r\n      },\r\n      rules: {\r\n        name: [\r\n          { required: true, message: '请输入项目名称', trigger: 'blur' }\r\n        ],\r\n        type: [\r\n          { required: true, message: '请选择项目类型', trigger: 'change' }\r\n        ]\r\n      }\r\n    }\r\n  },\r\n  created() {\r\n    this.fetchItemList()\r\n  },\r\n  methods: {\r\n    async fetchItemList() {\r\n      try {\r\n        this.loading = true\r\n        const response = await this.$http.get('/api/sports-items', {\r\n          params: {\r\n            keyword: this.searchKeyword,\r\n            page: this.currentPage - 1,\r\n            size: this.pageSize\r\n          }\r\n        })\r\n        \r\n        if (response.data.code === 200) {\r\n          const data = response.data.data\r\n          this.tableData = data.content\r\n          this.total = data.totalElements\r\n        } else {\r\n          this.$message.error(response.data.msg || '获取体测项目列表失败')\r\n        }\r\n      } catch (error) {\r\n        console.error('获取体测项目列表失败:', error)\r\n        this.$message.error('获取体测项目列表失败')\r\n      } finally {\r\n        this.loading = false\r\n      }\r\n    },\r\n    handleSearch() {\r\n      this.currentPage = 1\r\n      this.fetchItemList()\r\n    },\r\n    handleSizeChange(val) {\r\n      this.pageSize = val\r\n      this.fetchItemList()\r\n    },\r\n    handleCurrentChange(val) {\r\n      this.currentPage = val\r\n      this.fetchItemList()\r\n    },\r\n    handleAdd() {\r\n      this.dialogTitle = '添加体测项目'\r\n      this.form = {\r\n        name: '',\r\n        type: '',\r\n        description: '',\r\n        unit: '',\r\n        isActive: true\r\n      }\r\n      this.dialogVisible = true\r\n    },\r\n    handleEdit(row) {\r\n      this.dialogTitle = '编辑体测项目'\r\n      this.form = { ...row }\r\n      this.dialogVisible = true\r\n    },\r\n    async handleSubmit() {\r\n      this.$refs.form.validate(async valid => {\r\n        if (valid) {\r\n          try {\r\n            const url = this.form.id ? \r\n              `/api/sports-items/${this.form.id}` : \r\n              '/api/sports-items'\r\n            \r\n            const method = this.form.id ? 'put' : 'post'\r\n            const response = await this.$http[method](url, this.form)\r\n            \r\n            if (response.data.code === 200) {\r\n              this.$message.success(this.form.id ? '更新成功' : '添加成功')\r\n              this.dialogVisible = false\r\n              this.fetchItemList()\r\n            } else {\r\n              this.$message.error(response.data.msg || '操作失败')\r\n            }\r\n          } catch (error) {\r\n            console.error('操作失败:', error)\r\n            this.$message.error('操作失败')\r\n          }\r\n        }\r\n      })\r\n    },\r\n    async handleDelete(row) {\r\n      try {\r\n        await this.$confirm('确认删除该体测项目吗？', '提示', {\r\n          type: 'warning'\r\n        })\r\n        const response = await this.$http.delete(`/api/sports-items/${row.id}`)\r\n        if (response.data.code === 200) {\r\n          this.$message.success('删除成功')\r\n          this.fetchItemList()\r\n        } else {\r\n          this.$message.error(response.data.msg || '删除失败')\r\n        }\r\n      } catch (error) {\r\n        if (error !== 'cancel') {\r\n          console.error('删除失败:', error)\r\n          this.$message.error('删除失败')\r\n        }\r\n      }\r\n    },\r\n    async handleStatusChange(row) {\r\n      try {\r\n        const response = await this.$http.put(`/api/sports-items/${row.id}/status`, {\r\n          isActive: row.isActive\r\n        })\r\n        if (response.data.code === 200) {\r\n          this.$message.success(`${row.isActive ? '启用' : '禁用'}成功`)\r\n        } else {\r\n          row.isActive = !row.isActive // 恢复状态\r\n          this.$message.error(response.data.msg || '操作失败')\r\n        }\r\n      } catch (error) {\r\n        row.isActive = !row.isActive // 恢复状态\r\n        console.error('操作失败:', error)\r\n        this.$message.error('操作失败')\r\n      }\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.sports-item-management {\r\n  padding: 20px;\r\n}\r\n.operation-bar {\r\n  margin-bottom: 20px;\r\n  display: flex;\r\n  justify-content: space-between;\r\n  align-items: center;\r\n}\r\n.search-area {\r\n  display: flex;\r\n  align-items: center;\r\n}\r\n.pagination-container {\r\n  margin-top: 20px;\r\n  text-align: right;\r\n}\r\n</style> "],"mappings":"AA6GA;EACAA,IAAA;EACAC,KAAA;IACA;MACAC,aAAA;MACAC,SAAA;MACAC,OAAA;MACAC,WAAA;MACAC,QAAA;MACAC,KAAA;MACAC,aAAA;MACAC,WAAA;MACAC,IAAA;QACAV,IAAA;QACAW,IAAA;QACAC,WAAA;QACAC,IAAA;QACAC,QAAA;MACA;MACAC,KAAA;QACAf,IAAA,GACA;UAAAgB,QAAA;UAAAC,OAAA;UAAAC,OAAA;QAAA,EACA;QACAP,IAAA,GACA;UAAAK,QAAA;UAAAC,OAAA;UAAAC,OAAA;QAAA;MAEA;IACA;EACA;EACAC,QAAA;IACA,KAAAC,aAAA;EACA;EACAC,OAAA;IACA,MAAAD,cAAA;MACA;QACA,KAAAhB,OAAA;QACA,MAAAkB,QAAA,cAAAC,KAAA,CAAAC,GAAA;UACAC,MAAA;YACAC,OAAA,OAAAxB,aAAA;YACAyB,IAAA,OAAAtB,WAAA;YACAuB,IAAA,OAAAtB;UACA;QACA;QAEA,IAAAgB,QAAA,CAAArB,IAAA,CAAA4B,IAAA;UACA,MAAA5B,IAAA,GAAAqB,QAAA,CAAArB,IAAA,CAAAA,IAAA;UACA,KAAAE,SAAA,GAAAF,IAAA,CAAA6B,OAAA;UACA,KAAAvB,KAAA,GAAAN,IAAA,CAAA8B,aAAA;QACA;UACA,KAAAC,QAAA,CAAAC,KAAA,CAAAX,QAAA,CAAArB,IAAA,CAAAiC,GAAA;QACA;MACA,SAAAD,KAAA;QACAE,OAAA,CAAAF,KAAA,gBAAAA,KAAA;QACA,KAAAD,QAAA,CAAAC,KAAA;MACA;QACA,KAAA7B,OAAA;MACA;IACA;IACAgC,aAAA;MACA,KAAA/B,WAAA;MACA,KAAAe,aAAA;IACA;IACAiB,iBAAAC,GAAA;MACA,KAAAhC,QAAA,GAAAgC,GAAA;MACA,KAAAlB,aAAA;IACA;IACAmB,oBAAAD,GAAA;MACA,KAAAjC,WAAA,GAAAiC,GAAA;MACA,KAAAlB,aAAA;IACA;IACAoB,UAAA;MACA,KAAA/B,WAAA;