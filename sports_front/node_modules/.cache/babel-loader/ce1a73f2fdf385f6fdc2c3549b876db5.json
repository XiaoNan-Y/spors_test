asswordForm.validate(async valid => {\r\n        if (valid) {\r\n          try {\r\n            const changePasswordDTO = {\r\n              userId: this.userInfo.id,\r\n              oldPassword: this.passwordForm.oldPassword,\r\n              newPassword: this.passwordForm.newPassword\r\n            };\r\n            \r\n            const res = await this.$http.put('/api/auth/change-password', changePasswordDTO);\r\n            if (res.data.code === 200) {\r\n              this.$message.success('密码修改成功，请重新登录');\r\n              this.$store.dispatch('clearUser');\r\n              this.$router.push('/login');\r\n            } else {\r\n              this.$message.error(res.data.msg);\r\n            }\r\n          } catch (error) {\r\n            this.$message.error('修改密码失败');\r\n          }\r\n        }\r\n      });\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.profile {\r\n  padding: 20px;\r\n  display: grid;\r\n  grid-template-columns: 1fr 1fr;\r\n  gap: 20px;\r\n}\r\n.profile-card, .password-card {\r\n  min-height: 400px;\r\n}\r\n.button-group {\r\n  text-align: right;\r\n  margin-top: 20px;\r\n}\r\n</style> "],"mappings":";;AA2DA,SAAAA,QAAA;AAEA;EACAC,IAAA;EACAC,KAAA;IACA,MAAAC,uBAAA,GAAAA,CAAAC,IAAA,EAAAC,KAAA,EAAAC,QAAA;MACA,IAAAD,KAAA,UAAAE,YAAA,CAAAC,WAAA;QACAF,QAAA,KAAAG,KAAA;MACA;QACAH,QAAA;MACA;IACA;IACA;MACAI,SAAA;MACAH,YAAA;QACAI,WAAA;QACAH,WAAA;QACAI,eAAA;MACA;MACAC,aAAA;QACAF,WAAA,GACA;UAAAG,QAAA;UAAAC,OAAA;UAAAC,OAAA;QAAA,EACA;QACAR,WAAA,GACA;UAAAM,QAAA;UAAAC,OAAA;UAAAC,OAAA;QAAA,GACA;UAAAC,GAAA;UAAAF,OAAA;UAAAC,OAAA;QAAA,EACA;QACAJ,eAAA,GACA;UAAAE,QAAA;UAAAC,OAAA;UAAAC,OAAA;QAAA,GACA;UAAAE,SAAA,EAAAf,uBAAA;UAAAa,OAAA;QAAA;MAEA;IACA;EACA;EACAG,QAAA;IACA,GAAAnB,QAAA;MACAoB,QAAA,EAAAC,KAAA,IAAAA,KAAA,CAAAC;IACA;EACA;EACAC,OAAA;IACAC,WAAA;MACA,KAAAd,SAAA;IACA;IACA,MAAAe,WAAA;MACA;QACA,MAAAC,GAAA,cAAAC,KAAA,CAAAC,GAAA,iCAAAR,QAAA;QACA,IAAAM,GAAA,CAAAxB,IAAA,CAAA2B,IAAA;UACA,KAAAC,QAAA,CAAAC,OAAA;UACA,KAAArB,SAAA;UACA;UACA,KAAAsB,MAAA,CAAAC,