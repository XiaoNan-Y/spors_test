-form>\n    </div>\n\n    <el-table :data=\"appealList\" border style=\"width: 100%\">\n      <el-table-column prop=\"studentName\" label=\"学生姓名\" width=\"120\"/>\n      <el-table-column prop=\"studentNumber\" label=\"学号\" width=\"120\"/>\n      <el-table-column prop=\"className\" label=\"班级\" width=\"120\"/>\n      <el-table-column prop=\"testItem\" label=\"测试项目\" width=\"120\"/>\n      <el-table-column label=\"成绩信息\" width=\"180\">\n        <template slot-scope=\"scope\">\n          <div>原始成绩: {{ scope.row.originalScore }}</div>\n          <div>期望成绩: {{ scope.row.expectedScore }}</div>\n        </template>\n      </el-table-column>\n      <el-table-column prop=\"reason\" label=\"申诉理由\"/>\n      <el-table-column prop=\"createTime\" label=\"申请时间\" width=\"180\">\n        <template slot-scope=\"scope\">\n          {{ formatDateTime(scope.row.createTime) }}\n        </template>\n      </el-table-column>\n      <el-table-column prop=\"status\" label=\"状态\" width=\"100\">\n        <template slot-scope=\"scope\">\n          <el-tag :type=\"getStatusTag(scope.row.status)\">\n            {{ getStatusLabel(scope.row.status) }}\n          </el-tag>\n        </template>\n      </el-table-column>\n      <el-table-column label=\"操作\" width=\"200\" fixed=\"right\">\n        <template slot-scope=\"scope\">\n          <el-button \n            type=\"primary\" \n            size=\"small\" \n            @click=\"handleReview(scope.row)\"\n            :disabled=\"scope.row.status !== 'PENDING'\"\n          >\n            审核\n          </el-button>\n        </template>\n      </el-table-column>\n    </el-table>\n\n    <div class=\"pagination\">\n      <el-pagination\n        background\n        layout=\"total, prev, pager, next\"\n        :total=\"total\"\n        :page-size=\"pageSize\"\n        :current-page.sync=\"currentPage\"\n        @current-change=\"handlePageChange\"\n      />\n    </div>\n\n    <!-- 审核对话框 -->\n    <el-dialog\n      title=\"申诉审核\"\n      :visible.sync=\"reviewDialogVisible\"\n      width=\"50%\"\n    >\n      <el-form :model=\"reviewForm\" label-width=\"100px\">\n        <el-form-item label=\"学生信息\">\n          <div>姓名：{{ currentAppeal?.studentName }}</div>\n          <div>学号：{{ currentAppeal?.studentNumber }}</div>\n          <div>班级：{{ currentAppeal?.className }}</div>\n        </el-form-item>\n        <el-form-item label=\"测试项目\">\n          <div>{{ currentAppeal?.testItem }}</div>\n        </el-form-item>\n        <el-form-item label=\"原始成绩\">\n          <div>{{ currentAppeal?.originalScore }}</div>\n        </el-form-item>\n        <el-form-item label=\"申诉理由\">\n          <div class=\"appeal-reason\">{{ currentAppeal?.reason }}</div>\n        </el-form-item>\n        <el-form-item label=\"审核结果\">\n          <el-radio-group v-model=\"reviewForm.status\">\n            <el-radio label=\"APPROVED\">通过</el-radio>\n            <el-radio label=\"REJECTED\">驳回</el-radio>\n          </el-radio-group>\n        </el-form-item>\n        <el-form-item label=\"修正成绩\" v-if=\"reviewForm.status === 'APPROVED'\">\n          <el-input-number \n            v-model=\"reviewForm.correctedScore\" \n            :min=\"0\" \n            :max=\"100\"\n          />\n        </el-form-item>\n        <el-form-item label=\"审核意见\">\n          <el-input\n            type=\"textarea\"\n            v-model=\"reviewForm.comment\"\n            :rows=\"4\"\n            placeholder=\"请输入审核意见\"\n          />\n        </el-form-item>\n      </el-form>\n      <span slot=\"footer\" class=\"dialog-footer\">\n        <el-button @click=\"reviewDialogVisible = false\">取消</el-button>\n        <el-button type=\"primary\" @click=\"submitReview\">确定</el-button>\n      </span>\n    </el-dialog>\n  </div>\n</template>\n\n<script>\nexport default {\n  name: 'ScoreAppealReview',\n  data() {\n    return {\n      appealList: [],\n      total: 0,\n      currentPage: 1,\n      pageSize: 10,\n      reviewDialogVisible: false,\n      currentAppeal: null,\n      searchForm: {\n        status: ''\n      },\n      reviewForm: {\n        status: 'APPROVED',\n        correctedScore: null,\n        comment: ''\n      }\n    }\n  },\n\n  created() {\n    this.getAppeals()\n  },\n\n  methods: {\n    formatDateTime(datetime) {\n      if (!datetime) return '';\n      return new Date(datetime).toLocaleString();\n    },\n\n    async getAppeals() {\n      try {\n        console.log('Fetching appeals...');\n        const response = await this.$axios.get('/api/teacher/score-appeals', {\n          params: {\n            status: this.searchForm.status,\n            page: this.currentPage,\n            size: this.pageSize\n          }\n        });\n        \n        console.log('Response:', response.data);\n        \n        if (response.data.code === 200) {\n          this.appealList = response.data.data.content || [];\n          this.total = response.data.data.totalElements || 0;\n          console.log('Appeals loaded:', this.appealList);\n          \n          // 检查数据是否正确加载\n          if (this.appealList.length === 0) {\n            console.warn('No appeals found in response');\n          } else {\n            console.log('First appeal:', this.appealList[0]);\n          }\n        } else {\n          this.$message.error(response.data.message || '获取数据失败');\n        }\n      } catch (error) {\n        console.error('Error fetching appeals:', error);\n        this.$message.error('获取申诉列表失败: ' + (error.response?.data?.message || error.message));\n      }\n    },\n\n    searchAppeals() {\n      this.currentPage = 1\n      this.getAppeals()\n    },\n\n    handleReview(row) {\n      this.currentAppeal = row\n      this.reviewForm.status = 'APPROVED'\n      this.reviewForm.correctedScore = row.expectedScore || row.originalScore\n      this.reviewForm.comment = ''\n      this.reviewDialogVisible = true\n    },\n\n    async submitReview() {\n      if (!this.reviewForm.comment) {\n        this.$message.warning('请输入审核意见')\n        return\n      }\n\n      try {\n        const payload = {\n          status: this.reviewForm.status,\n          reviewComment: this.reviewForm.comment\n        };\n        \n        if (this.reviewForm.status === 'APPROVED') {\n          payload.expectedScore = this.reviewForm.correctedScore;\n        }\n        \n        console.log('Submitting review:', payload);\n        \n        const response = await this.$axios.put(\n          `/api/teacher/score-appeals/${this.currentAppeal.id}/review`,\n          payload\n        );\n        \n        if (response.data.code === 200) {\n          this.$message.success('审核成功');\n          this.reviewDialogVisible = false;\n          this.getAppeals();\n        } else {\n          this.$message.error(response.data.message || '审核失败');\n        }\n      } catch (error) {\n        console.error('Error submitting review:', error);\n        this.$message.error('审核失败: ' + (error.response?.data?.message || error.message));\n      }\n    },\n\n    handlePageChange(page) {\n      this.currentPage = page\n      this.getAppeals()\n    },\n\n    getStatusLabel(status) {\n      const statuses = {\n        'PENDING': '待处理',\n        'APPROVED': '已通过',\n        'REJECTED': '已驳回'\n      }\n      return statuses[status] || status\n    },\n\n    getStatusTag(status) {\n      const tags = {\n        'PENDING': 'warning',\n        'APPROVED': 'success',\n        'REJECTED': 'danger'\n      }\n      return tags[status] || ''\n    }\n  }\n}\n</script>\n\n<style scoped>\n.appeal-review {\n  padding: 20px;\n}\n\n.search-bar {\n  margin-bottom: 20px;\n}\n\n.pagination {\n  margin-top: 20px;\n  text-align: right;\n}\n\n.appeal-reason {\n  padding: 10px;\n  background-color: #f5f7fa;\n  border-radius: 4px;\n  min-height: 60px;\n}\n</style> "]}]}