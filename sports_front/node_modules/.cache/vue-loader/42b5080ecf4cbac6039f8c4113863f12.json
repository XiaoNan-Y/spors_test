on>\r\n    </div>\r\n\r\n    <el-table :data=\"teacherList\" style=\"width: 100%\" v-loading=\"loading\">\r\n      <el-table-column prop=\"username\" label=\"用户名\"></el-table-column>\r\n      <el-table-column prop=\"realName\" label=\"姓名\"></el-table-column>\r\n      <el-table-column prop=\"email\" label=\"邮箱\"></el-table-column>\r\n      <el-table-column prop=\"phone\" label=\"手机号\"></el-table-column>\r\n      <el-table-column label=\"操作\" width=\"250\">\r\n        <template slot-scope=\"scope\">\r\n          <el-button size=\"mini\" @click=\"handleEdit(scope.row)\">编辑</el-button>\r\n          <el-button size=\"mini\" type=\"warning\" @click=\"handleResetPassword(scope.row)\">重置密码</el-button>\r\n          <el-button size=\"mini\" type=\"danger\" @click=\"handleDelete(scope.row)\">删除</el-button>\r\n        </template>\r\n      </el-table-column>\r\n    </el-table>\r\n\r\n    <!-- 添加/编辑对话框 -->\r\n    <el-dialog :title=\"dialogTitle\" :visible.sync=\"dialogVisible\" width=\"500px\">\r\n      <el-form :model=\"form\" :rules=\"rules\" ref=\"form\" label-width=\"80px\">\r\n        <el-form-item label=\"用户名\" prop=\"username\">\r\n          <el-input v-model=\"form.username\" :disabled=\"form.id\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"姓名\" prop=\"realName\">\r\n          <el-input v-model=\"form.realName\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"邮箱\" prop=\"email\">\r\n          <el-input v-model=\"form.email\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"手机号\" prop=\"phone\">\r\n          <el-input v-model=\"form.phone\"></el-input>\r\n        </el-form-item>\r\n      </el-form>\r\n      <div slot=\"footer\">\r\n        <el-button @click=\"dialogVisible = false\">取消</el-button>\r\n        <el-button type=\"primary\" @click=\"handleSubmit\">确定</el-button>\r\n      </div>\r\n    </el-dialog>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: 'TeacherManagement',\r\n  data() {\r\n    return {\r\n      searchKeyword: '',\r\n      teacherList: [],\r\n      loading: false,\r\n      dialogVisible: false,\r\n      dialogTitle: '',\r\n      form: {\r\n        id: null,\r\n        username: '',\r\n        realName: '',\r\n        email: '',\r\n        phone: '',\r\n        userType: 'TEACHER'\r\n      },\r\n      rules: {\r\n        username: [\r\n          { required: true, message: '请输入用户名', trigger: 'blur' },\r\n          { min: 3, max: 20, message: '长度在 3 到 20 个字符', trigger: 'blur' }\r\n        ],\r\n        realName: [\r\n          { required: true, message: '请输入姓名', trigger: 'blur' }\r\n        ],\r\n        email: [\r\n          { type: 'email', message: '请输入正确的邮箱地址', trigger: 'blur' }\r\n        ],\r\n        phone: [\r\n          { pattern: /^1[3-9]\\d{9}$/, message: '请输入正确的手机号', trigger: 'blur' }\r\n        ]\r\n      }\r\n    }\r\n  },\r\n  created() {\r\n    this.fetchTeacherList()\r\n  },\r\n  methods: {\r\n    async fetchTeacherList() {\r\n      try {\r\n        this.loading = true\r\n        const res = await this.$http.get('/api/admin/users/list', {\r\n          params: {\r\n            userType: 'TEACHER',\r\n            keyword: this.searchKeyword\r\n          }\r\n        })\r\n        if (res.data.code === 200) {\r\n          this.teacherList = res.data.data\r\n        }\r\n      } catch (error) {\r\n        this.$message.error('获取教师列表失败')\r\n      } finally {\r\n        this.loading = false\r\n      }\r\n    },\r\n    handleSearch() {\r\n      this.fetchTeacherList()\r\n    },\r\n    handleAdd() {\r\n      this.dialogTitle = '添加教师'\r\n      this.form = {\r\n        id: null,\r\n        username: '',\r\n        realName: '',\r\n        email: '',\r\n        phone: '',\r\n        userType: 'TEACHER'\r\n      }\r\n      this.dialogVisible = true\r\n    },\r\n    handleEdit(row) {\r\n      this.dialogTitle = '编辑教师'\r\n      this.form = { ...row }\r\n      this.dialogVisible = true\r\n    },\r\n    async handleSubmit() {\r\n      this.$refs.form.validate(async valid => {\r\n        if (valid) {\r\n          try {\r\n            const url = this.form.id ? '/api/admin/users/update' : '/api/admin/users/add'\r\n            const res = await this.$http[this.form.id ? 'put' : 'post'](url, this.form)\r\n            if (res.data.code === 200) {\r\n              this.$message.success(res.data.msg)\r\n              this.dialogVisible = false\r\n              this.fetchTeacherList()\r\n            } else {\r\n              this.$message.error(res.data.msg)\r\n            }\r\n          } catch (error) {\r\n            this.$message.error('操作失败')\r\n          }\r\n        }\r\n      })\r\n    },\r\n    async handleDelete(row) {\r\n      try {\r\n        await this.$confirm('确认删除该教师吗？', '提示', {\r\n          type: 'warning'\r\n        })\r\n        const res = await this.$http.delete(`/api/admin/users/delete/${row.id}`)\r\n        if (res.data.code === 200) {\r\n          this.$message.success(res.data.msg)\r\n          this.fetchTeacherList()\r\n        } else {\r\n          this.$message.error(res.data.msg)\r\n        }\r\n      } catch (error) {\r\n        if (error !== 'cancel') {\r\n          this.$message.error('删除失败')\r\n        }\r\n      }\r\n    },\r\n    async handleResetPassword(row) {\r\n      try {\r\n        await this.$confirm('确认重置该教师的密码吗？重置后密码为：123456', '提示', {\r\n          type: 'warning'\r\n        })\r\n        const res = await this.$http.put(`/api/admin/users/reset-password/${row.id}`)\r\n        if (res.data.code === 200) {\r\n          this.$message.success(res.data.msg)\r\n        } else {\r\n          this.$message.error(res.data.msg)\r\n        }\r\n      } catch (error) {\r\n        if (error !== 'cancel') {\r\n          this.$message.error('重置密码失败')\r\n        }\r\n      }\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.teacher-management {\r\n  padding: 20px;\r\n}\r\n.operation-bar {\r\n  margin-bottom: 20px;\r\n  display: flex;\r\n  justify-content: space-between;\r\n}\r\n</style> "]}]}